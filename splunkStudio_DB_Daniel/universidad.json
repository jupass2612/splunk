    "dataSources": {
        "ds_GCK6fWSL": {
            "type": "ds.search",
            "options": {
                "query": "| dbxquery query=\"\n    SELECT DISTINCT \n        Plantel \n    FROM \n        `LotusNuevoActualizado`.`Hoja1`\n    WHERE \n        Universidad = 'UAM'\n    ORDER BY \n        Plantel ASC;\" connection=\"LotusFormNuevo\"",
                "queryParameters": {
                    "earliest": "0",
                    "latest": ""
                }
            },
            "name": "Selecciona un Plantel search"
        },
        "ds_GigjYRGK": {
            "type": "ds.search",
            "options": {
                "query": "| dbxquery query=\"\n    SELECT \n        `CualeslarazonporlaquenosegenerolaventaInscripcion` AS Razon,\n        COUNT(*) AS Total\n    FROM \n        `LotusNuevoActualizado`.`Hoja1`\n    WHERE \n        Universidad = 'UAM' \n        AND Plantel = '$plantel_token$'\n        AND `CualeslarazonporlaquenosegenerolaventaInscripcion` IS NOT NULL\n        AND `CualeslarazonporlaquenosegenerolaventaInscripcion` != 'No aplica'\n    GROUP BY \n        `CualeslarazonporlaquenosegenerolaventaInscripcion`\n    ORDER BY \n        Total DESC;\" connection=\"LotusFormNuevo\"",
                "queryParameters": {
                    "earliest": "-24h@h",
                    "latest": "now"
                }
            },
            "name": "Cierre de Venta - Razones de no ventas search"
        },
        "ds_FpqKRY0M": {
            "type": "ds.search",
            "options": {
                "query": "| dbxquery query=\"SELECT \n    `Serealizaunaventainscripcion` AS EstadoVenta,\n    COUNT(*) AS Total\nFROM \n    `LotusNuevoActualizado`.`Hoja1`\nWHERE \n    Universidad = 'UAM'\n    AND Plantel = '$plantel_token$'\nGROUP BY \n    `Serealizaunaventainscripcion`\nORDER BY \n    Total DESC;\n\" connection=\"LotusFormNuevo\"",
                "queryParameters": {
                    "earliest": "-24h@h",
                    "latest": "now"
                }
            },
            "name": "Cierre de Venta - Ventas Cerradas search"
        },
        "ds_0RM1VQ4J": {
            "type": "ds.search",
            "options": {
                "query": "| dbxquery query=\"SELECT \n    `Serealizaunaventainscripcion` AS EstadoVenta,\n    COUNT(*) AS Total\nFROM \n    `LotusNuevoActualizado`.`Hoja1`\nWHERE \n    Universidad = 'UAM'\n    AND Plantel = '$plantel_token$'\nGROUP BY \n    `Serealizaunaventainscripcion`\nORDER BY \n    Total DESC;\n\" connection=\"LotusFormNuevo\"",
                "queryParameters": {
                    "earliest": "-24h@h",
                    "latest": "now"
                }
            },
            "name": "Table search"
